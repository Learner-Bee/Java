package com.lily.testing;

public class ForTest {

	public static void main(String[] args) {
		int [] numbers={1,2,3,4,5};
		for(int x:numbers){
			if(x==4){
				break;
			}
			System.out.print(x+" ");
			
		}
		System.out.println("\n");
		String [] name={"lily","lucy" ,"mk"};
		for(String y:name){
			System.out.print(y+" ");
		}
		

	}

}
