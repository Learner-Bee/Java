package com.lily.collection;

import java.util.*;

public class CollectionRemove {

	public static void main(String[] args) {
		List<String> list=new ArrayList<String>();
		list.add("l");
		list.add("i");
		list.add("l");
		list.add("y");
		list.remove("l");
		System.out.println("删除l的集合为：");
		for(String i:list){
			System.out.println(i);
		}

	}

}
