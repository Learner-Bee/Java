package com.lily.method;

public class Continue {

	public static void main(String[] args) {
		StringBuffer sb=new StringBuffer("hello world");
		char c='l';
		int count=0;
		for(int i=0;i<sb.length();i++){
			if(sb.charAt(i)!=c){
				continue;
			}else{
				count++;
			}
		}
		System.out.println("字符串中的"+c+"元素总数为："+count);

	}

}
