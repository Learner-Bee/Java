package com.lily.string;

public class Format {

	public static void main(String[] args) {
		System.out.println(String.format("%2$05d",32,43));
		System.out.println(String.format("%1$5d",32,43));
		System.out.println(String.format("%1$-(5d",-32,43));
		System.out.println(String.format("%2$,05d",32,3443));
		//System.out.printf("’€ø€ «%d%% %n",100);
		//System.out.printf("%s",ty);
		

	}

}
