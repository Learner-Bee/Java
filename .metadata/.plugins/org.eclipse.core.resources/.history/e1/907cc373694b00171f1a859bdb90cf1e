package com.lily.directory;

import java.io.*;

public class FileList {

	public static void main(String[] args) {
		
		File f1=new File("E:/Learn/Java/test3/");
		String[] file=f1.list();
		if(file.length==0){
			System.out.println("该目录为空");
		}else{
			System.out.println("该目录下不存在指定文件");
		}

	}

}
