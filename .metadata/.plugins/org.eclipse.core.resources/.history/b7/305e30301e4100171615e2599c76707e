package com.lily.senior;

public class GenericMethodTest {
	
	public static <E> printArray(E[] array){
		for(E element:array){
			System.out.println(element);
		}
	}
	public static void main(String[] args){
		int[] i={1,3,4,5};
		String[] str={"lily","lucy","mk"};
		System.out.println("int类型数组：");
		printArray(i);
	}

}
