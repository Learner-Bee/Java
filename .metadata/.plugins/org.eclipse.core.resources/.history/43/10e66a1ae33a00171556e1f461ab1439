package com.lily.testing;

 class Super2Test {
   //private int n;
   Super2Test(){
	   System.out.println("SuperDemo2()");
   }
   Super2Test(int n){
	   System.out.println("SuperDemo2(int n)");	   
   }
}
 class Suber extends Super2Test{
	//private int n;
	Suber(){
		super(300);
		System.out.println("Suber()");
	}
	Suber(int n){
		System.out.println("Suber(int n)");
	}
	 
}
 public class SuperTest {

		public static void main(String[] args) {
			
				 Suber sb=new Suber();
				 Suber sb2=new Suber(200);
			 
		}

	}
 
