package com.lily.collection;

import java.util.*;

public class IndexOf {

	public static void main(String[] args) {
		List<String> l=new ArrayList<String>();
		l.add("l");
		l.add("i");
		l.add("l");
		l.add("y");
		List<String> l2=new ArrayList<String>();
		l2.add("l");
		if(l.indexOf(l2)!=-1){
			System.out.println("集合l中存在指定元素："+l2);
			System.out.println("集合l中指定元素最后出现的位置为："+l.lastIndexOf(l2));
		}

	}

}
