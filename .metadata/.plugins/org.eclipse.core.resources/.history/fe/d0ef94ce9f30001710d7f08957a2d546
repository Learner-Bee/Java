package com.lily.testing;

public class TestArray {

	public static void main(String[] args) {
		double[] myList=new double[size];
		myList[0]=1;
		myList[1]=2;
		myList[2]=2;
		myList[3]=2;
		myList[4]=2;
		double totle=0;
		for(int i=0; i<size;i++){
