package com.lily.string;

public class Split {

	public static void main(String[] args) {
		String str="gg-dd-aa";
		String[] a=str.split("-");
		//String[] b=str.split("\\/");
		//String[] c=str.split("\\|");
		//String[] d=str.split("?|/");
		for(int i=0;i<a.length;i++){
		   System.out.println(a[i]);
		}
	}

}
