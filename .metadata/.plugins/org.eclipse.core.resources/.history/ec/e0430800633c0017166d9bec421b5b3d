package com.lily.testing;

public class EmployeeP {
	//定义私有变量，限制只能在该类中使用
	private String name;
	private String address;
	private int id;
	//创建有参构造函数
	public EmployeeP(String name,String address,int id){
		System.out.println("EmployeeP构造函数");
		this.name=name;
		this.address=address;
		this.id=id;
	}

}
