package com.lily.method;

public class Swtich {

	public static void main(String[] args) {
		Car c;
		c=Car.lucy;
		switch(c){
		case lily:
			System.out.println("lily");
			break;
		case lucy:
			System.out.println("lucy");
			break;
		case mk:
			System.out.println("mk");
			break;
		}

	}

}
enum Car{
	lily,lucy,mk;
}