package com.lily.file;

import java.io.BufferedReader;
import java.io.FileReader;

public class Reader {

	public static void main(String[] args) {
		try{
			BufferedReader bf=new BufferedReader(new FileReader("test.txt"));
			int a;
			while((a=bf.read())!=-1){
				System.out.print((char)a);
			}
		}catch(Exception e){}

	}

}
