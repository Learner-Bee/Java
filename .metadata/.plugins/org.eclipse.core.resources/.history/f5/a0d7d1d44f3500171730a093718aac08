package com.lily.testing;

public class Finalize {

	public static void main(String[] args) {
		Cake c1=new Cake(1);
		Cake c2=new Cake(2);
		Cake c3=new Cake(3);
		
		c2=null;
		c3=null;
		System.gc();

	}
	public static void  Cake(int c){
		System.out.println("Cake object "+c+" is created");
	}

	
}
